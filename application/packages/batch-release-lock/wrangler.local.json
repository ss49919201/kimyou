{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "kimyou-batch-release-lock",
  "main": "src/index.ts",
  "compatibility_date": "2025-02-04",
  "compatibility_flags": ["nodejs_compat"],
  "triggers": {
    "crons": [
      "0 * * * *"
    ]
  },
  "d1_databases": [
    {
      "binding": "D1",
      "database_name" : "kimyou",
      "database_id" : "n/a"
    }
  ]
}